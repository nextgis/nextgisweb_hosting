server {
	listen 80; 
	# listen [::]:80 ipv6only=on;

    server_name console.gis.to;

    location / {

        resolver 192.168.18.6;
        resolver_timeout 10s;

        set $up "http://127.0.0.1:5000";

        proxy_pass $up;
    }

    location /redis/ {

        resolver 192.168.18.6;
        resolver_timeout 10s;

        set $up "http://redis.ngw:9090";

        proxy_pass $up;
    }
}

